    "babel",
    "transform",
    "plugin",
    "modules",
    "esm"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoshRosenstein/babel-plugin-transform-module-imports.git"
  },
  "bugs": {
    "url": "https://github.com/JoshRosenstein/babel-plugin-transform-module-imports/issues"
  },
  "homepage": "https://github.com/JoshRosenstein/babel-plugin-transform-module-imports#readme",
  "dependencies": {
