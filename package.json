    "@types/lodash.snakecase": "^4.1.4",
    "@babel/core": "^7.1.2",
    "@types/node": "^10.12.6",
    "mocha": "^5.2.0",
    "ts-node": "7.0.1",
    "typescript": "^3.1.1"
  }
